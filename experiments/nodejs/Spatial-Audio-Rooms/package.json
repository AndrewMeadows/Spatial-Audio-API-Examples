{
  "name": "spatial-audio-rooms",
  "version": "0.1.0",
  "description": "A complex demo application for High Fidelity's Spatial Audio API which integrates many of the API's features as well as Twilio Video Conferencing.",
  "main": "src/index.ts",
  "scripts": {
    "client": "webpack --env dev",
    "client:dev": "webpack --env dev",
    "client:prod": "webpack --env prod",
    "server": "node -r esm ./src/server/index.js dev",
    "server:dev": "node -r esm ./src/server/index.js dev",
    "server:prod": "node -r esm ./src/server/index.js prod",
    "audiobots": "node -r esm ./src/server/audioBots.js",
    "discord": "tsc --p ./src/discordBot/ && node ./src/discordBot/dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/highfidelity/Spatial-Audio-API-Examples.git"
